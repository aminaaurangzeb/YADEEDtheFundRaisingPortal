
<!DOCTYPE html>

@using WebApplication6.Models;

@model List<cases>
<html>
<head>
    <script src="~/js/checkfields.js">
    </script>
    <meta name="viewport" content="width=device-width" />
    <title>signin</title>
    <link rel="stylesheet" type="text/css" href="~/Content/StyleSheet.css" />
</head>
<body>
    <br />
    <heading2>REMOVE CASE HERE</heading2>
    <div>
        <styling>

            <table border="1">
                <thead><tr><td>Case ID</td> <td>Name</td> <td>Catergory</td><td>Target Amount</td></tr></thead>

                @foreach (cases C in Model)
                {
                    <tr>
                        <td>@C.id</td>
                        <td>@C.name</td>
                        <td>@C.category</td>
                        <td>@C.target</td>
                    </tr>
                    <br />
                    <br />
                }

            </table>
        </styling>
        <form method="post" onsubmit="return addEventCheck();" action="./authenticate15">
            <fieldset class="outerrec">
                <div class="inputlabel" id="f">
                    <br />
                    <label> Case Id:</label> <br />
                    <enter>
                        <select name="id">
                            @foreach (cases C in Model)

                            {
                                <option value="@C.id">@C.id</option>
                            }

                        </select>
                    </enter>
                    <br />
                    <br />

                </div>
                <enter>
                    <input type="submit" id="loginButton" value="Remove" /> <label id="status"></label>
                </enter>
                <br />
                <br />
            </fieldset>
        </form>

    </div>
    <form method="post" onsubmit="return checkForFieldsSignIn();" action="./authenticate12">
        <logout>

            <enter>
                <input type="submit" id="returnButton" value="RETURN" onclick="checkforemptyfields()">    <label id="status"></label>
            </enter>


        </logout>

    </form>
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
